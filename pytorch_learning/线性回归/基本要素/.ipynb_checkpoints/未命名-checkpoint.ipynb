    "import torch\n",
    "from time import time\n",
    "\n",
    "a=torch.ones(1000)\n",
    "b=torch.ones(1000)\n",
    "\n",
    "start=time()\n",
    "c=torch.ones(1000)\n",
    "for i in range (1000):\n",
    "    c[i]=a[i]+b[i]\n",
    "print(time()-start)"
